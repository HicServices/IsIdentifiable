<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <AssemblyName>IsIdentifiable</AssemblyName>
	  <authors>Universities of Dundee and Edinburgh</authors>
	  <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
	  <PackageProjectUrl>https://github.com/SMI/IsIdentifiable</PackageProjectUrl>
	  <OutputType>Library</OutputType>
	  <description>Library for spotting identifiable data in flat files, dicom files and relational databases (Sql Server, My Sql, Oracle).</description>
	  <PackageTags>Identifiable,Detection,Validation,NLP,OCR,Natural Language Processing,Optical Character Recognition</PackageTags>
	  <copyright>Copyright 2019</copyright>
	<TargetFramework>netstandard2.1</TargetFramework>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	<GenerateDocumentationFile>True</GenerateDocumentationFile>
    <LangVersion>10.0</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Rules.template.yaml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\SharedAssemblyInfo.cs" Link="SharedAssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
     <None Pack="true" PackagePath="lib/netstandard2.1/x64"  Include="lib\libtesseract.so.4.0.1-centos7" Link="x64\libtesseract41.so"/>
     <None Pack="true" PackagePath="lib/netstandard2.1/x64"  Include="lib\libleptonica.so.1.78.0-minimal-centos7" Link="x64\libleptonica-1.78.0.so"/>
     <None Pack="true" PackagePath="lib/netstandard2.1/x64"  Include="lib\libleptonica.so.1.78.0-minimal-centos7" Link="x64\libleptonica.so.5.3.0"/>
     <None Pack="true" PackagePath="lib/netstandard2.1"  Include="Rules.template.yaml"/>
     <None Pack="true" PackagePath="lib/netstandard2.1"  Include="../Reviewer/theme.yaml"/>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.9.1" />
    <PackageReference Include="CsvHelper" Version="27.2.1" />
    <PackageReference Include="Equ" Version="2.3.0" />
    <PackageReference Include="fo-dicom.Imaging.ImageSharp" Version="5.0.2" />
    <PackageReference Include="HIC.DicomTypeTranslation" Version="4.0.0" />
    <PackageReference Include="HIC.FAnsiSql" Version="2.0.4" />
	<PackageReference Include="Magick.NET-Q16-AnyCPU" Version="11.1.2" />
	<PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="6.0.1" />
    <PackageReference Include="NLog" Version="5.0.0" />
    <PackageReference Include="System.IO.Abstractions" Version="17.0.12" />
    <PackageReference Include="System.IO.FileSystem" Version="4.3.0" />
    <PackageReference Include="Tesseract" Version="4.1.0-beta1" />
  </ItemGroup>
</Project>
