    <tags>Identifiable,Detection,Validation,NLP,OCR,Natural Language Processing,Optical Character Recognition</tags>
    <dependencies>
      <group targetFramework="net6.0">
		  <dependency id="CommandLineParser" version="2.9.1" />
		  <dependency id="Equ" version="2.3.0" />
		  <dependency id="CsvHelper" version="27.2.1" />
		  <dependency id="fo-dicom.Imaging.ImageSharp" version="5.0.2" />
		  <dependency id="HIC.DicomTypeTranslation" version="4.0.0" />
		  <dependency id="HIC.FAnsiSql" version="2.0.4" />
		  <dependency id="Magick.NET-Q16-AnyCPU" version="11.1.2" />
		  <dependency id="Microsoft.Extensions.Caching.Memory" version="6.0.1" />
		  <dependency id="NLog" version="5.0.0" />
		  <dependency id="System.IO.Abstractions" version="17.0.12" />
		  <dependency id="System.IO.FileSystem" version="4.3.0" />
		  <dependency id="Tesseract" version="4.1.0-beta1" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\netstandard2.1\**" target="lib\netstandard2.1" />
  </files>
</package>
